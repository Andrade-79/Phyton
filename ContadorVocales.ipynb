      "source": [
        "frase = input(\"Pon lo qué quieras: \")\n",
        "\n",
        "vocales = \"aeiou\"\n",
        "contadorVocales = 0\n",
        "\n",
        "for letra in frase.lower():\n",
        "    if letra in vocales:\n",
        "        contadorVocales += 1\n",
        "\n",
        "\n",
        "\n",
        "print(f\"Tu frase fue: {frase}\")\n",
        "print(f\"Cantidad de vocales:  {contadorVocales}\")"
      ]
